{
  "name": "point-of-sale",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:ci-e2e-server": "npm run dev && vite --mode test http://localhost:3000 'cypress run --headless'",
    "dev-with-instrumentation": "npm run test:ci-e2e-server",
    "test:cypress": "cypress run && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/material": "^5.16.0",
    "cypress": "^13.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "remove": "^0.1.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.41",
    "@cypress/react18": "^2.0.1",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "nyc": "^17.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-istanbul": "^6.0.2"
  }
}
